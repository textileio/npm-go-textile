{
  "name": "@textile/go-textile",
  "version": "0.6.3",
  "description": "Textile binary installation module for Node",
  "main": "dist/index.js",
  "bin": {
    "textile": "bin/textile"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "link": "node ./dist/link.js",
    "prepare": "npm run build",
    "install": "npm run build && npm run link",
    "test": "jest --no-watchman --config jest.config.json",
    "lint": "eslint --fix --ext .mjs,.js,.jsx,.ts,.tsx src && echo 'Lint complete.'",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 3.0.0",
    "yarn": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/npm-go-textile.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/npm-go-textile/issues"
  },
  "keywords": [
    "textile",
    "install",
    "IPFS",
    "binary"
  ],
  "author": "textile.io",
  "license": "MIT",
  "dependencies": {
    "@textile/go-textile-dep": "0.6.4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "execa": "^2.0.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.6.8",
    "rimraf": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
    "prettier-eslint": "^8.8.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.4"
  }
}
